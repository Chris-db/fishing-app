{
  "version": 2,
  "builds": [
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/bundles/(.*)",
      "dest": "/bundles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
